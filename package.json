{
  "name": "pump-fun-mock-trader",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "prisma generate && next build",
    "start": "next start -H 0.0.0.0",
    "lint": "next lint",
    "db:start": "sudo systemctl start postgresql && npm run db:wait",
    "db:stop": "sudo systemctl stop postgresql",
    "db:status": "sudo systemctl status postgresql",
    "db:reset": "npm run db:setup && npm run db:wait && npm run db:migrate:dev && npm run db:seed",
    "db:setup": "sudo -u postgres psql -c \"DROP DATABASE IF EXISTS autotrader;\" && sudo -u postgres psql -c \"DROP USER IF EXISTS autotrader;\" && sudo -u postgres psql -c \"CREATE USER autotrader WITH PASSWORD 'autotrader_password' CREATEDB;\" && sudo -u postgres psql -c \"CREATE DATABASE autotrader OWNER autotrader;\"",
    "db:wait": "node scripts/wait-for-db.js",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "setup": "npm install && npm run db:wait && npm run db:migrate:dev && npm run db:seed",
    "postinstall": "prisma generate",
    "ingest:trades": "tsx src/scripts/ingest-trades.ts",
    "ingest:tokens": "tsx src/scripts/ingest-tokens.ts",
    "fetch:sol-price": "tsx src/scripts/fetch-sol-price.ts",
    "fix:lamports": "tsx src/scripts/fix-lamports-trades.ts",
    "backfill:prices": "tsx src/scripts/backfill-token-prices.ts",
    "clean:wallets": "tsx src/scripts/cleanup-wallets.ts",
    "pm2:start": "pm2 start ecosystem.config.js --only autotrader-web",
    "pm2:start:all": "pm2 start ecosystem.config.js",
    "pm2:start:ingest": "pm2 start ecosystem.config.js --only autotrader-ingest-trades",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@prisma/client": "^5.9.0",
    "@tanstack/react-query": "^5.17.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^3.0.6",
    "eventemitter3": "^5.0.1",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.24",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "prisma": "^5.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
